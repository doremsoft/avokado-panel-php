<body class="layout layout-vertical layout-left-navigation layout-below-toolbar layout-below-footer">
{% block modals %}{% endblock %}
<main>
    <div id="wrapper">
        <aside id="aside" class="aside aside-left" data-fuse-bar="aside" data-fuse-bar-media-step="md"
               data-fuse-bar-position="left">
            <div class="aside-content bg-primary-700 text-auto">

                {% if user.bayi == 0 %}
                    {% include 'nav.twig' %}
                {% else %}

                    {% include 'bayi-nav.twig' %}

                {% endif %}


            </div>

        </aside>

        <div id='dipapreloader' class="dipaloading"></div>

        <div class="content-wrapper">

            {% if mobile != 1 %}

                <nav id="toolbar" class="bg-fuse-toolbar MuiPaper-elevation4-31 ">

                    <div class="row no-gutters align-items-center flex-nowrap">

                        <div class="col">

                            <div class="row no-gutters align-items-center flex-nowrap">

                                <button type="button" class="toggle-aside-button btn btn-icon d-block d-lg-none"
                                        data-fuse-bar-toggle="aside">
                                    <i class="icon icon-menu"></i>
                                </button>

                                <div class="toolbar-separator d-block d-lg-none"></div>

                                <div class="shortcuts-wrapper row no-gutters align-items-center px-0 px-sm-2">

                                    <div class="shortcuts row no-gutters align-items-center d-none d-md-flex">

                                        <a href="{{ url }}/etkinlik/takvim" class="shortcut-button btn btn-icon mx-1">
                                            <i class="secondary-text mat-icon notranslate material-icons mat-icon-no-color ng-star-inserted"
                                               role="img" aria-hidden="true">today</i>
                                        </a>

                                        <a href="{{ url }}/satislar/iki-tarih-gore-satislar"
                                           class="shortcut-button btn btn-icon mx-1">
                                            <i class="secondary-text mat-icon notranslate material-icons mat-icon-no-color ng-star-inserted">trending_up</i>
                                        </a>

                                        <a href="{{ url }}/cari/list" class="shortcut-button btn btn-icon mx-1">
                                            <i class="secondary-text mat-icon notranslate material-icons mat-icon-no-color ng-star-inserted"
                                               role="img" aria-hidden="true">account_box</i>
                                        </a>

                                    </div>


                                    <div class="add-shortcut-menu-button dropdown px-1 px-sm-2">

                                        <div role="button" id="dropdownNotifiyMenu" class="dropdown-toggle btn btn-icon"
                                             data-fuse-bar-toggle="quick-panel-sidebar">
                                            <i class="secondary-text s-48 mat-icon notranslate material-icons mat-icon-no-color  "
                                               id="bildirimleri-icon" data-alert="0" role="img" aria-hidden="true">notifications_active</i>

                                            <!-- notifications_active notifications_none -->
                                        </div>


                                    </div>


                                </div>


                            </div>
                        </div>

                        <div class="col-auto">

                            <div class="row no-gutters align-items-center justify-content-end">

                                <div class="toolbar-separator"></div>

                                <button type="button" onclick="sweetCariArama();" class="search-button btn btn-icon">
                                    <i class="icon icon-magnify"></i>
                                </button>

                                <div class="toolbar-separator"></div>

                                <div class="user-menu-button dropdown">

                                    <div class="dropdown-toggle ripple row align-items-center no-gutters px-2 px-sm-4"
                                         role="button" id="dropdownUserMenu" data-toggle="dropdown" aria-haspopup="true"
                                         aria-expanded="false">
                                        <div class="avatar-wrapper">

                                            {% if user.image == "noimage.jpg" %}
                                                {% if user.gender == "male" %}
                                                    <img src="{{ asset('images/user/avatar-2.jpg') }}" alt="user"
                                                         class="avatar"/>
                                                {% else %}
                                                    <img src="{{ asset('images/user/avatar-1.jpg') }}" alt="user"
                                                         class="avatar"/>
                                                {% endif %}
                                            {% else %}
                                                <img src="{{ media('userpicture/',user.image) }}" alt="user"
                                                     class="avatar"/>
                                            {% endif %}

                                            <i class="status text-green icon-checkbox-marked-circle s-4"></i>
                                        </div>
                                        <span class="username mx-3 d-none d-md-block">{{ user.name }} {{ user.surname }}</span>
                                    </div>

                                    <div class="dropdown-menu" aria-labelledby="dropdownUserMenu">

                                        <a class="dropdown-item" href="{{ url }}/user/edit">
                                            <div class="row no-gutters align-items-center flex-nowrap">
                                                <i class="icon-account"></i>
                                                <span class="px-3">Bilgi Güncelle</span>
                                            </div>
                                        </a>

                                        <a class="dropdown-item" href="{{ url }}/user/password-edit">
                                            <div class="row no-gutters align-items-center flex-nowrap">
                                                <i class="icon-lock"></i>
                                                <span class="px-3">Şifre İşlemi</span>
                                            </div>
                                        </a>


                                        <div class="dropdown-divider"></div>

                                        <a class="dropdown-item" href="#" onclick="event.preventDefault();
                            document.getElementById('logout-form').submit();">
                                            <div class="row no-gutters align-items-center flex-nowrap">
                                                <i class="icon-logout"></i>
                                                <span class="px-3">Oturum Kapat</span>
                                            </div>
                                        </a>

                                    </div>
                                </div>


                            </div>
                        </div>
                    </div>
                </nav>

            {% endif %}


            <div class="content custom-scrollbar">
                {% block content %}{% endblock %}
            </div>

        </div>


        <div class="quick-panel-sidebar custom-scrollbar" style="max-width: 280px;" fuse-cloak
             data-fuse-bar="quick-panel-sidebar" data-fuse-bar-position="right">
            <header class="row no-gutters align-items-center justify-content-between bg-secondary text-auto p-4">
                <div  data-fuse-bar-toggle="quick-panel-sidebar" style="cursor: pointer;" class=" title h6">
                    <i  class=" secondary-text s-48 mat-icon notranslate material-icons mat-icon-no-color" role="img" aria-hidden="true">close</i>


                    <label style="font-size: 24px;">Bildirimler</label></div>
                <div><a class="more " href="{{ url }}/bildirim/liste">Bütün Bildirimler...</a></div>
            </header>
            <div class="content activities " id="anasayfa-bildirimleri">
                {% for bildirim in sonbildirimler %}
                    <div class="activity row no-gutters bildirimlistesi bildirim-kalem p-4" data-bildirim-id="{{ bildirim.id }}"data-okunmayan-bildirim="0">

                        <div style="width: 100%;">


                                    {% set bildirim_url = "#" %}
                                    {% if  bildirim.tip == 0 %}
                                    {% elseif  bildirim.tip == 1 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% elseif  bildirim.tip == 2 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% elseif  bildirim.tip == 3 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% elseif  bildirim.tip == 5 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% elseif  bildirim.tip == 6 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% elseif  bildirim.tip == 7 %}
                                        {% set bildirim_url %}{{ url }}/post/show/{{ bildirim.islem_id }}{% endset %}
                                    {% endif %}


                            <div style="width: 30px;float: left;">


                                {% if bildirim.bildirim_icon == "" %}
                                    <i class="secondary-text s-48 mat-icon notranslate material-icons mat-icon-no-color" role="img" aria-hidden="true">keyboard_arrow_right</i>
                                {% else %}
                                <i class="secondary-text s-48 mat-icon notranslate material-icons mat-icon-no-color" role="img" aria-hidden="true">{{bildirim.bildirim_icon}}</i>
{% endif %}
                            </div>


                            <div  style=" width: 100%; padding-right: 170px; margin-right: -150px;float: left;">

                                     <span class="bildirim-baslik ">{{bildirim.bildirim_baslik}} </span> <br>

                                    <span class="message"><a class="bildirim bildirim-mesaj" href="{{ bildirim_url }}">
                                            {{ get_html( bildirim.bildirim_mesaj) }}
                                        </a>
                                    </span>



                            </div>

                            <div style="width: 120px;float: left;">

   <span class="message">
                                    <span class="time text-muted bildirim-mesaj">{{ tarih(bildirim.created_date , 'd-m-Y H:i') }}</span>
                                    </span>
                            </div>


                        </div>
                    </div>

                {% endfor %}
            </div>
        </div>


    </div>
</main>



